<UserControl x:Class="StockAnalyzerApp.CustomControl.PortfolioDlg.TradeDlgs.TradeManager.TradeManagerControlxaml"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StockAnalyzerApp.CustomControl.PortfolioDlg.TradeDlgs.TradeManager"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Language="en-UK"
             d:Background="Bisque"
             d:DataContext="{d:DesignInstance Type=local:TradeManagerViewModel, IsDesignTimeCreatable=False}">
    <Grid>
        <Label HorizontalAlignment="Center" VerticalAlignment="Center" Content="{Binding StockSerie.StockName}" FontSize="20"/>
    </Grid>
</UserControl>
